{
	"name": "Vtools",
	"version": "1.1",
	"description": "Extension for skipping ads and setting custom playback speeds.",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Vtools"
	}
}